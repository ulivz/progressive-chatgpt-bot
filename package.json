{
  "name": "progressive-chatgpt-bot",
  "description": "A progressive ChatGPT feishu bot, made to assist my girlfriend's work.",
  "version": "0.1.0",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "keywords": [
    "workflow",
    "speedy",
    "speedy-js",
    "publish",
    "release",
    "npm publish"
  ],
  "scripts": {
    "bootstrap": "pnpm i --prefer-offline",
    "clean": "rm -rf cjs esm",
    "dev": "npm run build:cjs -- --watch",
    "build": "tsc -p tsconfig.json --module commonjs --outDir lib",
    "prepublishOnly": "npm run build",
    "lint": "eslint -c .eslintrc.js src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "cov": "jest --coverage",
    "release": "quick-publish"
  },
  "dependencies": {
    "@larksuiteoapi/node-sdk": "^1.11.1",
    "aircode": "^0.2.7",
    "chatgpt": "^4.4.1"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.7",
    "lodash.throttle": "^4.1.1",
    "tslib": "2.4.0",
    "@types/jest": "28.1.7",
    "@types/node": "18.7.6",
    "eslint": "7",
    "eslint-config-typescript-library": "0.2.4",
    "jest": "28.1.3",
    "jest-cli": "28.1.3",
    "jest-serializer-path": "0.1.15",
    "prettier": "2.7.1",
    "quick-publish": "0.6.0",
    "rollup": "^3.17.0",
    "throttle-debounce": "^5.0.0",
    "ts-jest": "28.0.8",
    "typescript": "4.7.4"
  },
  "files": [
    "bin",
    "esm",
    "!esm/*.tsbuildinfo",
    "!esm/*.map",
    "lib",
    "!lib/*.tsbuildinfo",
    "!lib/*.map",
    "types",
    "*.d.ts"
  ]
}
